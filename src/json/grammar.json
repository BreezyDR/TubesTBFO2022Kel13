{
    "terminal":
    {
        "keywords":
        [
            "break",
            "const",
            "case",
            "catch",
            "continue",
            "default",
            "delete",
            "else",
            "false",
            "finally",
            "for",
            "function",
            "if",
            "let",
            "null",
            "return",
            "switch",
            "throw",
            "try",
            "true",
            "var",
            "while"
        ],
        "brackets":
        {
            "squared-brackets"  :    ["[", "]"],
            "rounded-brackets"  :    ["(", ")"],
            "curly-brackets"    :    ["{", "}"]
        },
        "operator":{
            "math-ops":["+", "-"],
            "assignment":["="]
        }
    },

    "grammar":{
        "operation":[
            "operation + variable",
            "variable + variable"
        ],
        "declaration":[
            "let statement",
            "const statement"
        ],
        "rhs":[
            "operation"
        ],
        "lhs":[
            "variable broken"
        ],
        "statement":[
            "lhs = rhs"
        ],
        "absolute_start":[
            "statement"
        ]
        
        
        
    }
}